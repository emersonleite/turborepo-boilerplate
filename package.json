{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:webapp": "turbo run build --filter=@eva/webapp",
    "build:webapp2": "turbo run build --filter=@eva/webapp2",
    "build:webapp3": "turbo run build --filter=@eva/webapp3",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:ui": "turbo run test --filter=@eva/ui",
    "prepare": "turbo run prepare"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "eva-monorepo",
  "packageManager": "yarn@1.22.18",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}